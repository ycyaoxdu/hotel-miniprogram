.header{
  width: 100%;
  height: 90rpx;
  border: 1px solid red;
}

.container{
  padding: 0 26rpx;
  box-sizing: border-box;
}

.hdCon{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100%;
}
.hdCon navigator{
  height: 50rpx;
}
.hdCon image{
  height: 100%;
}


.footer{
  padding: 30rpx 0;
  background: #f2f2f2 ;
  text-align: center;
}
.footer .pic{
  width: 90rpx;
}
.footer .txt{
  font-size: 25rpx;
  color: #999;
}